{
  "name": "p003-angular-nestjs-webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:all": "npm run build:client && npm run build:server",
    "build:all:watch": "npm run build:client:watch && npm run build:server:watch",
    "build:client": "ng build",
    "build:client:watch": "ng build --watch --configuration production",
    "build:server": "tsc --project server/tsconfig.server.json",
    "build:server:watch": "tsc --project server/tsconfig.server.json --watch",
    "start:server": "node dist/main --watch",
    "serve": "npm run build:all && npm run start:server",
    "test": "ng test --watch=false --codeCoverage=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@nestjs/common": "^8.0.5",
    "@nestjs/core": "^8.0.5",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "rxjs": "^6.5.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.17",
    "jasmine-core": "^3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "^10.1.0",
    "typescript": "~4.2.3"
  }
}
