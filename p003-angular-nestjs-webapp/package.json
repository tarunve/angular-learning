{
  "name": "p003-angular-nestjs-webapp",
  "version": "0.0.0",
  "scripts": {
    "serve": "npm-run-all build:server --parallel build:client:watch build:server:watch start:debug",
    "build:client": "ng build --configuration production",
    "build:client:watch": "ng build --watch --configuration production",
    "build:server": "tsc --project tsconfig.server.json",
    "build:server:watch": "tsc --project tsconfig.server.json --watch",
    "start": "node dist/server",
    "start:debug": "nodemon --inspect --delay 2 --watch src --watch dist --watch server-src dist/server.js",
    "test:unit": "ng test --watch false",
    "test:unit:watch": "npm run test:unit -- --configuration=watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.5",
    "@angular/common": "~12.2.5",
    "@angular/compiler": "~12.2.5",
    "@angular/core": "~12.2.5",
    "@angular/forms": "~12.2.5",
    "@angular/platform-browser": "~12.2.5",
    "@angular/platform-browser-dynamic": "~12.2.5",
    "@angular/router": "~12.2.5",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.5",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.5",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@nrwl/jest": "^12.9.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.2.0",
    "jest-html-reporter": "^3.4.1",
    "jest-junit": "^12.2.0",
    "jest-preset-angular": "^9.0.7",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  }
}
